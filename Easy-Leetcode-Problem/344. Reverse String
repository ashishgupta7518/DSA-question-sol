

/*    My c++ Solution Of Leetcode Problem 344. Reverse String    */




class Solution {
public:
    void reverseString(vector<char>& s) {
        int n = s.size();
        int i=0;
        int j=n-1;

        while(i<=j){
            swap(s[i] , s[j]);
                i++;
                j--;
        }
        
    }
};
